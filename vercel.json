{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/",
            "dest": "server.js"
        },
        {
            "src": "/login.html",
            "dest": "server.js"
        },
        {
            "src": "/dashboard.html",
            "dest": "server.js"
        }
    ]
}